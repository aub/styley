%ul
  - @types.each do |type|
    %li
      = type.name
      = link_to('Edit', edit_type_path(type))
    - if type.children
      %ul
        - type.children.each do |child|
          %li
            = child.name 
            = link_to('Edit', edit_type_path(child))

%label{ :for => 'zoom_level' } Zoom Level: 
= select_tag(:zoom_level, options_for_select((1..20).map { |i| [i, i] }))

%label{ :for => 'center_lat' } Center Lat:
= text_field_tag(:center_lat, '51.5081')

%label{ :for => 'center_lon' } Center Lon:
= text_field_tag(:center_lon, '-0.0878')

= submit_tag('Create Sample', :id => 'create_sample_button');

#sample_image
  %h1 Sample goes here

